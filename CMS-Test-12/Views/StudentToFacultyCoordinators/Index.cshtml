@model IEnumerable<CMS_Test_12.Models.StudentToFacultyCoordinator>
@{
	ViewBag.Title = "Index";
}

<h2>Index</h2>

@Html.ActionLink("Assign Student to Coordinator Faculty", "Create")
@using (Html.BeginForm("Index", "StudentToFacultyCoordinators", FormMethod.Get))
{
	<p>
		Find by Email: @Html.TextBox("SearchString")
		<input type="submit" value="Search" />
	</p>
}
<table class="table table-hover">
	<tr>
		<th>Id</th>
		<th>Student Name</th>
		<th>Faculty Coordinator Name</th>
		<th>Delete</th>

	</tr>
	@foreach (var item in Model)
	{
		<tr>
			<th>@item.Id</th>
			<th>@item.ManageStudent.Name</th>
			<th>@item.CoordinatorToFaculty.Id</th>=
			<td>@Html.ActionLink("Delete", "Delete", new { id = item.Id })</td>
		</tr>
	}
</table>
